{
  "name": "gatsby",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/gatsby",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "dependsOn": ["^build"],
      "options": {
        "main": "packages/gatsby/index.ts",
        "tsConfig": "packages/gatsby/tsconfig.json",
        "outputPath": "dist/packages/gatsby",
        "updateBuildableProjectDepsInPackageJson": false,
        "assets": [
          "packages/gatsby/*.md",
          {
            "input": "packages/gatsby",
            "glob": "**/*.json",
            "ignore": ["**/tsconfig*.json", "project.json"],
            "output": "/"
          },
          {
            "input": "packages/gatsby",
            "glob": "**/*.d.ts",
            "output": "/"
          },
          "LICENSE"
        ]
      }
    },
    "publish": {
      "command": "node tools/scripts/publish.mjs gatsby {args.ver} {args.tag}"
    }
  },
  "tags": []
}
